"\n"
"  show [<ifname>]\n"
"\n"
"  connect <ifname>\n"
"\n"
"  disconnect <ifname>\n"
"\n"
"  wifi [list [ifname <ifname>] [bssid <BSSID>]]\n"
"\n"
