"save-confirmation yes | no  [default: yes]\n"
"prompt-color <0-8>          [default: 0]\n"
"  0 = normal\n"
"  1 = [30mblack[0m\n"
"  2 = [31mred[0m\n"
"  3 = [32mgreen[0m\n"
"  4 = [33myellow[0m\n"
"  5 = [34mblue[0m\n"
"  6 = [35mmagenta[0m\n"
"  7 = [36mcyan[0m\n"
"  8 = [37mwhite[0m\n"
"\n"
"Examples: nmcli> nmcli status-line yes\n"
"          nmcli> nmcli save-confirmation no\n"
"          nmcli> nmcli prompt-color 3\n"

