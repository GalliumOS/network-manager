"                  [password <PPPoE-lösenord>]\n"
"                  [service <PPPoE-tjänstenamn>]\n"
"                  [mtu <MTU>]\n"
"                  [mac <MAC-adress>]\n"
"\n"
"    gsm:          apn <APN>\n"
"                  [user <användarnamn>]\n"
"                  [password <lösenord>]\n"
"\n"
"    cdma:         [user <användarnamn>]\n"
