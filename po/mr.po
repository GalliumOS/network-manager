"active-backup = 1\n"
"balance-xor   = 2\n"
"broadcast     = 3\n"
"802.3ad       = 4\n"
"balance-tlb   = 5\n"
"balance-alb   = 6\n"
"\n"
"उदाहरणार्थ: mode=2,miimon=120\n"

